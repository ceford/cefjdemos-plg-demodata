<?xml version="1.0" encoding="UTF-8"?>
<extension type="plugin" group="demodata" method="upgrade">
	<name>plg_demodata_features101</name>
	<author>Joomla! Project</author>
	<creationDate>2018-07</creationDate>
	<copyright>(C) 2018 Open Source Matters, Inc.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<authorEmail>admin@joomla.org</authorEmail>
	<authorUrl>www.joomla.org</authorUrl>
	<version>0.1.0</version>
	<description>PLG_DEMODATA_FEATURES101_XML_DESCRIPTION</description>
	<namespace path="src">Cefjdemos\Plugin\DemoData\Features101</namespace>
	<files>
		<folder>assets</folder>
		<folder>language</folder>
		<folder plugin="features101">services</folder>
		<folder>src</folder>
	</files>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="languages"
					type="text"
					label="PLG_DEMODATA_FEATURES101_LANGUAGES_LABEL"
					description="PLG_DEMODATA_FEATURES101_LANGUAGES_DESC"
					default="en-GB"
				/>
				<field
					name="is_installed"
					type="radio"
					layout="joomla.form.field.radio.switcher"
					label="PLG_DEMODATA_FEATURES101_IS_INSTALLED_LABEL"
					description="PLG_DEMODATA_FEATURES101_IS_INSTALLED_DESC"
					default="0"
					filter="integer"
					readonly="readonly"
					>
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field
					name="users"
					type="text"
					label="PLG_DEMODATA_FEATURES101_USERS_LABEL"
					description="PLG_DEMODATA_FEATURES101_USERS_DESC"
					readonly="readonly"
				/>
				<field
					name="usergroups"
					type="text"
					label="PLG_DEMODATA_FEATURES101_USERGROUPS_LABEL"
					description="PLG_DEMODATA_FEATURES101_USERGROUPS_DESC"
					readonly="readonly"
				/>
				<field
					name="credentials"
					type="text"
					label="PLG_DEMODATA_FEATURES101_CREDENTIALS_LABEL"
					description="PLG_DEMODATA_FEATURES101_CREDENTIALS_DESC"
					readonly="readonly"
				/>
				<field
					name="categories"
					type="text"
					label="PLG_DEMODATA_FEATURES101_CATEGORIES_LABEL"
					description="PLG_DEMODATA_FEATURES101_CATEGORIES_DESC"
					readonly="readonly"
				/>
				<field
					name="tags"
					type="text"
					label="PLG_DEMODATA_FEATURES101_TAGS_LABEL"
					description="PLG_DEMODATA_FEATURES101_TAGS_DESC"
					readonly="readonly"
				/>
				<field
					name="banners"
					type="text"
					label="PLG_DEMODATA_FEATURES101_BANNERS_LABEL"
					description="PLG_DEMODATA_FEATURES101_BANNERS_DESC"
					readonly="readonly"
				/>
				<field
					name="fieldgroups"
					type="text"
					label="PLG_DEMODATA_FEATURES101_FIELDGROUPS_LABEL"
					description="PLG_DEMODATA_FEATURES101_FIELDGROUPS_DESC"
					readonly="readonly"
				/>
				<field
					name="fields"
					type="text"
					label="PLG_DEMODATA_FEATURES101_FIELDS_LABEL"
					description="PLG_DEMODATA_FEATURES101_FIELDS_DESC"
					readonly="readonly"
				/>
				<field
					name="workflows"
					type="text"
					label="PLG_DEMODATA_FEATURES101_WORKFLOWS_LABEL"
					description="PLG_DEMODATA_FEATURES101_WORKFLOWS_DESC"
					readonly="readonly"
				/>
				<field
					name="workflowstages"
					type="text"
					label="PLG_DEMODATA_FEATURES101_WORKFLOWSTAGES_LABEL"
					description="PLG_DEMODATA_FEATURES101_WORKFLOWSTAGES_DESC"
					readonly="readonly"
				/>
				<field
					name="workflowtransitions"
					type="text"
					label="PLG_DEMODATA_FEATURES101_WORKFLOWTRANSITIONS_LABEL"
					description="PLG_DEMODATA_FEATURES101_WORKFLOWTRANSITIONS_DESC"
					readonly="readonly"
				/>
				<field
					name="articles"
					type="text"
					label="PLG_DEMODATA_FEATURES101_ARTICLES_LABEL"
					description="PLG_DEMODATA_FEATURES101_ARTICLES_DESC"
					readonly="readonly"
				/>
				<field
					name="contacts"
					type="text"
					label="PLG_DEMODATA_FEATURES101_CONTACTS_LABEL"
					description="PLG_DEMODATA_FEATURES101_CONTACTS_DESC"
					readonly="readonly"
				/>
				<field
					name="newsfeeds"
					type="text"
					label="PLG_DEMODATA_FEATURES101_NEWSFEEDS_LABEL"
					description="PLG_DEMODATA_FEATURES101_NEWSFEEDS_DESC"
					readonly="readonly"
				/>
				<field
					name="menus"
					type="text"
					label="PLG_DEMODATA_FEATURES101_MENUS_LABEL"
					description="PLG_DEMODATA_FEATURES101_MENUS_DESC"
					readonly="readonly"
				/>
				<field
					name="menuitems"
					type="text"
					label="PLG_DEMODATA_FEATURES101_MENUITEMS_LABEL"
					description="PLG_DEMODATA_FEATURES101_MENUITEMS_DESC"
					readonly="readonly"
				/>
				<field
					name="associations"
					type="text"
					label="PLG_DEMODATA_FEATURES101_ASSOCIATIONS_LABEL"
					description="PLG_DEMODATA_FEATURES101_ASSOCIATIONS_DESC"
					readonly="readonly"
				/>
				<field
					name="modules"
					type="text"
					label="PLG_DEMODATA_FEATURES101_MODULES_LABEL"
					description="PLG_DEMODATA_FEATURES101_MODULES_DESC"
					readonly="readonly"
				/>
			</fieldset>
		</fields>
	</config>
</extension>
